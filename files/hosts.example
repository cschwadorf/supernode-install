# This is the default ansible 'hosts' file.
#
# It should live in /etc/ansible/hosts
#
#   - Comments begin with the '#' character
#   - Blank lines are ignored
#   - Groups of hosts are delimited by [header] elements
#   - You can enter hostnames or ip addresses
#   - A hostname/ip can be a member of multiple groups

[freifunk]

[freifunk_sn:children]
voreifel1
voreifel2

[freifunk_sn_l2tp:children]
voreifel1
voreifel2

[freifunk_sn:vars]
ansible_ssh_port=22
ansible_ssh_user=root
sn_mtu=1312
sn_l2tp_tb_port=53842
sn_fqdn=freifunk-rhein-sieg.net
static_dhcp_repo=https://github.com/Freifunk-Troisdorf/static-dhcp.git
root_password_file=/home/thomas/Freifunk/Supernode/root_pwd.yml
slack_token_file=/home/thomas/Freifunk/Supernode/slack_token.yml
communitymac="a2:8c:ae:7f:f7"
communityname=voreifel

[voreifel1]
voreifel1.freifunk-rhein-sieg.net

[voreifel1:vars]
sn_number=1
sn_hostname=voreifel1
sn_dhcp_range=10.152.65.0 10.152.78.254
sn_mesh_IPv6=2a03:2260:121:1000::1
sn_mesh_IPv6_net=2a03:2260:121:1000::/64
sn_mesh_IPv6_xfer=2a03:2260:121:1000::2
sn_mesh_IPv4=10.152.64.1
sn_mesh_IPv4_brcast=10.152.79.255
sn_mesh_IPv4_net=10.152.64.0
sn_mesh_IPv4_xfer=10.152.64.2
sn_mesh_MAC=a2:8c:ae:6f:f7:01
ul_mesh_MAC=a2:8c:ae:6f:f7:10
sn_ffrl_IPv4=185.66.193.101
sn_local_exit=1
sn_interface_name=ens18
yanic_domain=voreifel1

[voreifel2]
voreifel2.freifunk-rhein-sieg.net

[voreifel2:vars]
sn_number=2
sn_hostname=voreifel2
sn_dhcp_range=10.152.81.0 10.152.94.254
sn_mesh_IPv6=2a03:2260:121:2000::1
sn_mesh_IPv6_net=2a03:2260:121:2000::/64
sn_mesh_IPv6_xfer=2a03:2260:121:2000::2
sn_mesh_IPv4=10.152.80.1
sn_mesh_IPv4_brcast=10.152.95.255
sn_mesh_IPv4_net=10.152.80.0
sn_mesh_IPv4_xfer=10.152.80.2
sn_mesh_MAC=a2:8c:ae:6f:f7:02
ul_mesh_MAC=a2:8c:ae:6f:f7:20
sn_ffrl_IPv4=185.66.193.102
sn_local_exit=1
sn_interface_name=ens18
yanic_domain=voreifel2
